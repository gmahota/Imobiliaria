<h2>Novas Respostas paras Fotos</h2>

<%= simple_form_for @foto , :url => { :action => :create } do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :descricao %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>


<%= link_to 'Back', fotosresposta_path %>
