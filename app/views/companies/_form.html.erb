<%= simple_form_for(@company) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nome %>
    <%= f.input :email %>
    <%= f.input :morada %>
    <%= f.input :telefone %>
    <%= f.input :celular %>
    <%= f.input :facebook %>
    <%= f.input :skype %>
    <%= f.input :twitter %>
    <%= f.input :latitude %>
    <%= f.input :longitude %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
